{  
  	"name": "coffee",
  	"manifest_version":2,
	"version": "1.0",  
	"description": "coffee test",  
	"browser_action": {  
	   "default_icon": "icon.png" ,
	   "default_title": "My Task List",
    	"default_popup": "popup.html"
	 },
	 "background": {
	    "page": "background.html"
	  },
	 "content_scripts": [{
	     "matches": ["http://*/*","https://*/*"], 
	     "js": ["js/jquery-1.9.1.min.js", "js/test.js"], 
	     "run_at": "document_start",
	     "all_frames": true 
	}],
  	"permissions": [   
   	 "tabs", "http://*/*","https://*/*"  
  	]   
}